{
  "name": "docstore-sharelatex",
  "version": "0.1.2",
  "description": "A CRUD API for handling text documents in projects",
  "author": "ShareLaTeX <team@sharelatex>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharelatex/docstore-sharelatex.git"
  },
  "scripts": {
    "start": "node $NODE_APP_OPTIONS app.js",
    "test:acceptance:_run": "mocha --recursive --reporter spec --timeout 15000 --exit $@ test/acceptance/js",
    "test:acceptance": "npm run test:acceptance:_run -- --grep=$MOCHA_GREP",
    "test:unit:_run": "mocha --recursive --reporter spec $@ test/unit/js",
    "test:unit": "npm run test:unit:_run -- --grep=$MOCHA_GREP",
    "nodemon": "nodemon --config nodemon.json",
    "lint": "node_modules/.bin/eslint .",
    "format": "node_modules/.bin/prettier-eslint $PWD/'**/*.js' --list-different",
    "format:fix": "node_modules/.bin/prettier-eslint $PWD/'**/*.js' --write"
  },
  "dependencies": {
    "async": "^2.6.3",
    "aws-sdk": "^2.658.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "logger-sharelatex": "git+https://github.com/das7pad/logger-sharelatex.git#bef2078daae2433a43e61db306e301398199daa0",
    "metrics-sharelatex": "git+https://github.com/das7pad/metrics-sharelatex.git#471b0a35d8125818393738cb95702367add6d203",
    "mongojs": "git+https://github.com/das7pad/mongojs.git#e39e97bf289435e2d111c6201d3306b6b776d31e",
    "request": "~2.88.2",
    "settings-sharelatex": "git+https://github.com/das7pad/settings-sharelatex.git#d0797834438df2d102798a64602adf2f71ac8e48",
    "underscore": "~1.9.2"
  },
  "devDependencies": {
    "chai": "~4.2.0",
    "mocha": "^7.1.1",
    "sandboxed-module": "~2.0.3",
    "sinon": "~9.0.1"
  },
  "engines": {
    "node": "~6.14.1"
  }
}
